<script type="text/javascript">

    $(function () {

      $('#quickSearchTxt').keypress(function (e) {
        if ((e.keyCode || e.which) == 13) {
          $('#quickSearchBt').click();
          return false;
        } else if ((e.keyCode || e.which) == 8 && $('#quickSearchTxt').val().length == 1)
          $('#quickSearchTip').fadeIn();
        else
          $('#quickSearchTip').fadeOut();
      });

      function doSearchBt() {
        if ((typeof (s2c) != 'undefined') && (typeof (s2c.Explore) != 'undefined') &&
            (typeof (s2c.Explore.applyAllFilters) != 'undefined')) {
          s2c.Explore.applyAllFilters();
        } else {
          searchBtn();
        }
      }

      // JQueryUI Initialization elements
      $("#quickSearchBt").click(function () {
        doSearchBt();
      });
      $("#searchBt").click(function () {
        doSearchBt();
      });

      $('#quickSearchTxt').focus();
    });
  </script>


